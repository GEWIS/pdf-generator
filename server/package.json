{
  "name": "pdf",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ignore-path .gitignore --ignore-path .formatignore",
    "lint:fix": "eslint src --ignore-path .gitignore --ignore-path .formatignore --fix",
    "prettier": "prettier --ignore-path .gitignore --ignore-path .formatignore --check src/",
    "prettier:fix": "prettier --ignore-path .gitignore --ignore-path .formatignore --write src/",
    "build": "tsc",
    "serve": "node build/index.js",
    "watch:routes": "nodemon -w src/controllers -e ts -x \"yarn run generate:middleware\"",
    "watch:build": "nodemon -w src -e ts --ignore routes.ts -x \"tsc && yarn run serve\"",
    "dev": "concurrently \"yarn run watch:routes\" \"yarn run watch:build\"",
    "generate:middleware": "tsoa routes && tsoa spec",
    "templates:sync": "git clone git@github.com:GEWIS/latex-templates.git templates"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "chai": "^5.0.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.4.0",
    "chai-sorted": "^0.2.0",
    "chai-swag": "^0.1.9",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "joi": "^17.13.0",
    "node-latex": "^3.1.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/chai-sorted": "^0.2.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.11.6",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.4",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.1",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4"
  }
}
